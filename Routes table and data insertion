-- Inserting data into Proutes table by pulling TransportationModeID and ClassOfServiceID from respective tables
 
INSERT INTO Proutes (OriginCode, DestinationCode, TransportationModeID, ClassOfServiceID, Cost, HoursTraveled)

SELECT

    'ISS', -- Replace with your OriginCode

    'BWN', -- Replace with your DestinationCode

    TM.TransportationModeID,

    CS.ClassOfServiceID,

    100000, -- Replace with the Cost value

    48 -- Replace with the HoursTraveled value

FROM

    TransportationMode TM

INNER JOIN

    ClassesOfService CS ON TM.Description = 'Dragon' AND CS.Description = 'Premium'
 
UNION ALL
 
SELECT

    'BWN', -- Replace with your OriginCode

    'ISS', -- Replace with your DestinationCode

    TM.TransportationModeID,

    CS.ClassOfServiceID,

    400000, -- Replace with the Cost value

    96 -- Replace with the HoursTraveled value

FROM

    TransportationMode TM

INNER JOIN

    ClassesOfService CS ON TM.Description = 'Falcon' AND CS.Description = 'Premium'
 


